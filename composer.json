{
  "name": "cloudonix/cloudonix-php",
  "description": "PHP Class Library for interacting with Cloudonix APIs and Voice Developer APIs",
  "type": "library",
  "keywords": [
	"cloudonix",
	"cxml"
  ],
  "authors": [
	{
	  "name": "Nir Simionovich",
	  "email": "nirs@cloudonix.io",
	  "homepage": "https://cloudonix.io",
	  "role": "Packager"
	}
  ],
  "homepage": "https://github.com/cloudonix/cloudonix-php",
  "license": "MIT",
  "autoload": {
	"classmap": [
	  "./src/"
	],
	"psr-4": {
	  "Cloudonix\\": [
		"./src/"
	  ],
	  "Cloudonix\\Verbs\\": [
		"./src/CxML/"
	  ],
	  "Cloudonix\\Helpers\\": [
		"./src/Helpers/"
	  ],
	  "Cloudonix\\Collections\\": [
		"./src/Collections/"
	  ],
	  "Cloudonix\\Entities\\": [
		"./src/Entities/"
	  ]
	}
  },
  "authors": [
	{
	  "name": "Cloudonix Team",
	  "email": "support@cloudonix.io"
	}
  ],
  "require": {
	"php": ">=8.2",
	"ramsey/uuid": ">=4.7",
	"guzzlehttp/guzzle": ">=7.5",
	"phpfastcache/phpfastcache": ">=9.1.3",
	"monolog/monolog": ">=3.3.1",
    "vlucas/phpdotenv": "^5.5",
    "hackzilla/password-generator": "^1.6",
    "spatie/array-to-xml": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
	"shalvah/clara": ">=3.1"
  }
}
